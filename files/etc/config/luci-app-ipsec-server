
config service 'ipsec'
	option secret 'ipsec'
	option l2tp_localip '192.168.101.1'
	option l2tp_remoteip '192.168.101.10-20'
	option clientip '10.0.0.100/24'
	option enabled '0'
	option l2tp_enable '0'

config ipsec_users
	option enabled '1'
	option username 'Al'
	option password 'hlhexild'

config l2tp_users
	option enabled '1'
	option username 'Al'
	option password 'hlhexild'

